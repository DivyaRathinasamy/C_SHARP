@model Dish

<a href="/dishes">View All Dishes</a>


<h2>@Model.Name</h2>
<div>@Model.Chef</div>
<div>@Model.Calories</div>
<div>@Model.Tastiness</div>
<div>@Model.Description</div>


<a href="/dishes/@Model.DishId/edit">Edit</a>

<form
    asp-controller ="Dish"
    asp-action = "DeleteDish"
    asp-route-deletedDishId ="@Model.DishId"
    method="POST"



>

    <button class="btn-outline-danger">Delete</button>



</form>